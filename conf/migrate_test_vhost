Listen 4321
Listen 1212
# Vhost activation
Listen 1234

<VirtualHost *:1234>
	DocumentRoot /var/www
    <Directory />
        Options FollowSymLinks
        AllowOverride None
		Order allow,deny
		allow from all
    </Directory>
</VirtualHost>


<VirtualHost *:4321>
	DocumentRoot /var/www

    <Directory />
    	Options FollowSymLinks
        AllowOverride None
    </Directory>

    <Directory /var/www/dup_test/cgi/>
        AllowOverride None
        Order allow,deny
        allow from all
    </Directory>

    ScriptAlias /print_content /var/www/dup_test/cgi/print_content.cgi
</VirtualHost>

<VirtualHost *:1212>
	DocumentRoot /var/www

    <Directory />
    	Options FollowSymLinks
        AllowOverride None
    </Directory>

    <Directory /var/www/dup_test/cgi/>
        AllowOverride None
        Order allow,deny
        allow from all
    </Directory>

    ScriptAlias /local/print_content /var/www/dup_test/cgi/print_content.cgi
</VirtualHost>
