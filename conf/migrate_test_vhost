Listen 4321
Listen 1212
# Vhost activation
Listen 1234

<VirtualHost *:4321>
    ScriptAlias /print_content /var/www/dup_test/cgi/print_content.cgi
	Order allow,deny
	Allow from all
</VirtualHost>

<VirtualHost *:1212>
    ScriptAlias /local/print_content /var/www/dup_test/cgi/print_content.cgi
	Order allow,deny
	Allow from all
</VirtualHost>
